		{
			"name": "GET maps - verify that exist",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"function areSetsEqual(a, b) {\r",
							"  if (a.size !== b.size) {\r",
							"    return false;\r",
							"  }\r",
							"\r",
							"  return Array.from(a).every(element => {\r",
							"    return b.has(element);\r",
							"  });\r",
							"}\r",
							"pm.test(\"Request is successful with a status code of 200\", function () {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Check that it returns an array\", function () {\r",
							"  var jsonData = pm.response.json();\r",
							"  pm.expect(jsonData).to.be.an(\"array\");\r",
							"});\r",
							"\r",
							"pm.test(\"Check that there are 3 maps\", function () {\r",
							"  var jsonData = pm.response.json();\r",
							"  pm.expect(jsonData.length).to.eq(3)\r",
							"});\r",
							"\r",
							"pm.test(\"Verify first map schema\", function () {\r",
							"  var jsonData = pm.response.json();\r",
							"  var fstOBOMap = jsonData[0];\r",
							"  var pointsSet = new Set(fstOBOMap.Points);\r",
							"  var compSet = new Set([{\"X\": 653.31, \"Y\": 564.943},{\"X\": 332.75, \"Y\": 90.98},{\"X\": 522.23,\"Y\": 905.52},{\"X\": 113.23,\"Y\": 180.22}]);\r",
							"  pm.expect(areSetsEqual(pointsSet, compSet));\r",
							"});\r",
							"\r",
							"pm.test(\"Verify second map schema\", function () {\r",
							"  var jsonData = pm.response.json();\r",
							"  var fstOBOMap = jsonData[1];\r",
							"  var pointsSet = new Set(fstOBOMap.Points);\r",
							"  var compSet = new Set([{\"X\": 653.31,\"Y\": 564.943},{\"X\": 332.75,\"Y\": 90.98},{\"X\": 522.23,\"Y\": 905.52},{\"X\": 113.23,\"Y\": 180.22},{\"X\": 87.154,\"Y\": 463.943},{\r",
							"\"X\": 97.881,\"Y\": 18.88},{\"X\": 18.44,\"Y\": 230.0},{\"X\": 144.43,\"Y\": 563.39}]);\r",
							"  pm.expect(areSetsEqual(pointsSet, compSet));\r",
							"});\r",
							"\r",
							"pm.test(\"Verify third map schema\", function () {\r",
							"  var jsonData = pm.response.json();\r",
							"  var fstOBOMap = jsonData[2];\r",
							"  var pointsSet = new Set(fstOBOMap.Points);\r",
							"  var compSet = new Set([{\"X\": 985.42,\"Y\": 45.943},{\"X\": 862.65,\"Y\": 872.98},{\"X\": 731.64,\"Y\": 124.4}]);\r",
							"  pm.expect(areSetsEqual(pointsSet, compSet));\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
